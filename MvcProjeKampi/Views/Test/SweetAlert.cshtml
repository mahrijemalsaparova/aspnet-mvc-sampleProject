
@{
    ViewBag.Title = "SweetAlert";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />

<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<script src="~/Scripts/jquery-3.4.1.js"></script>


<button class="btn btn-primary" id="btn1">Button1</button>
<button class="btn btn-warning" id="btn2">Button1</button>
<button class="btn btn-danger" id="btn3">Button1</button>

<script>
    $('#btn1').click(function () {
        swal("Merhaba Mvc Proje Kampı");
    });
</script>
<script>
    $('#btn2').click(function () {
        swal("Merhaba Mvc Proje Kampı", "Ders 96");
    });
</script>
<script>
    $('#btn3').click(function () {
        swal("Tebrikler!", "Ekeleme gerçekleşti", "success");
    });
</script>